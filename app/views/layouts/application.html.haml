!!!
%html
  %head
    %title [TITLE]
    = stylesheet_link_tag    'application'
    = javascript_include_tag 'application'
    = csrf_meta_tags
    = yield :head
    = render 'layouts/fonts'
  %body
    #wrap
      = render 'layouts/header'
      .container
        .headline
          = render 'flashes'
          = yield
        .clearfix
    #push
    #footer
      .clearfix
      / %footer
      /   = render 'layouts/footer'
